    "yiisoft/yii2-queue": "~2.3.0",
    "yiisoft/yii2-shell": "~2.0.0",
    "google/apiclient": "^2.0",
    "kartik-v/yii2-grid": "@dev",
    "kartik-v/yii2-mpdf": "@dev",
    "kartik-v/yii2-widget-select2": "~2.0.0",
